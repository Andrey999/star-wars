{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "esnext",
    "module": "commonjs",
    "outDir": "./build/frontend",
    "baseUrl": "src",
    "rootDir": "src",
    "allowJs": false,
    "declaration": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "alwaysStrict": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    // "paths": {
    //   "components/*": ["components/*"],
    //   "constants/*": ["constants/*"],
    //   "pages/*": ["pages/*"],
    //   "hoc/*": ["hoc/*"],
    //   "routes/*": ["routes/*"],
    //   "utils/*": ["utils/*"]
    // }
    // "rootDir": "src",
    // "esModuleInterop": true,
    // "outDir": "./build/",
    // "preserveConstEnums": true,
    // "removeComments": true,
  },
  "include": [
    "./src/index.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}